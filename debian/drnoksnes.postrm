#!/bin/sh
# postrm script for drnoksnes
#
# see: dh_installdeb(1)

set -e

case "$1" in
	purge)
		gconftool-2 --recursive-unset /apps/maemo/drnoksnes || true
	;;

    remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0

